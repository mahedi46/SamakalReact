RewriteRule ^([^&]+)/drafts/([^&]+)/$ drafts.php?nssl=$2&%{QUERY_STRING} [NC,L]

RewriteRule ^([^&]+)/([0-9]+)$ details.php?nssl=$2&%{QUERY_STRING} [NC,L]
RewriteRule ^([^&]+)/([0-9]+)/$ details.php?nssl=$2&%{QUERY_STRING} [NC,L]

RewriteRule ^([^&]+)/([0-9]+)/([^&]+)$ details.php?nssl=$2&%{QUERY_STRING} [NC,L]
RewriteRule ^([^&]+)/([0-9]+)/([^&]+)/$ details.php?nssl=$2&%{QUERY_STRING} [NC,L]

