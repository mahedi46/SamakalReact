RewriteRule ^writer/([0-9]+)/([^&]+)/$ reporter.php?newstype=$1&%{QUERY_STRING} [NC,L]



RewriteRule ^tags/([^&]+)$ tags.php?tagname=$1 [NC,L]
RewriteRule ^tags/([^&]+)/$ tags.php?tagname=$1 [NC,L]

RewriteRule ^reactions/([^&]+)$ reactions.php?reactionname=$1 [NC,L]
RewriteRule ^reactions/([^&]+)/$ reactions.php?reactionname=$1 [NC,L]

#RewriteRule ^writers$ writers.php
